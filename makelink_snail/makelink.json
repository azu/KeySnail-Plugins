[
    {
        "title": "plain",
        "format": "%TITLE%\n%URL%"
    },
    {
        "title": "HTML",
        "format": "<a href=\"%URL%\" title=\"%TITLE%\">%TEXT%</a>"
    },
    {
        "title": "引用",
        "format": "<blockquote cite=\"%url%\"\n title=\"%title%\">\n<p>%text_br%</p>\n<cite>\n <a href=\"%url%\">%title%</a>\n</cite>\n</blockquote>"
    },
    {
        "title": "JSer",
        "format": "<blockquote title=\"%title%\">\n<p class=\"jser-sitelink\"><strong>%text_br%</strong><br /> <a href=\"%url%\" title=\"%text%\">%url%</a></p>\n</blockquote>"
    },
    {
        "title": "Markdown",
        "format": "[%text%](%url% \"%text%\")",
        "plain": true
    },
    {
        "title": "@Markdown",
        "format": "[%text%]: %url%  \"%title%\"",
        "plain": true
    },
    {
        "title": "rst",
        "format": "`%text% <%url%>`_",
        "plain": true
    },
    {
        "title": "@rst",
        "format": ".. _`%text%`: %url%",
        "plain": true
    }
]